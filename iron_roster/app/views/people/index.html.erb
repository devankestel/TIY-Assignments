<table>
  <th>
      <td>id</td>
      <td>name</td>
      <td>subject</td>
      <td>github_account</td>
      <td>created_at</td>
      <td>updated_at</td>
      <td>edit link</td>
      <td>delete student</td>
  </th>
  <% if @people.any? %>
    <% @people.each do |person| %>   
      <tr>
        <td><%= person.id %></td>
        <td><%= person.name %></td>
        <td><%= person.subject %></td>
        <td><%= person.github_account %></td>
        <td><%= person.created_at %></td>
        <td><%= person.updated_at %></td>
        <td><%= link_to "Edit", people_edit_path(person.id)%></td>
        <td><%= link_to "Delete", people_destroy_path(person.id)%></td>
      </tr>
    <% end %>
  <% end %>
</table>
<%= link_to "New Student Form", people_new_path%>